{
    "dart.flutterSdkPath": null,
    "dart.lineLength": 80,
    "dart.formatOnSave": true,
    "dart.analysisExcludedFolders": [
        "portfolio/build/**",
        "portfolio/.dart_tool/**"
    ],
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "files.exclude": {
        "**/.dart_tool": true,
        "**/build": true,
        "main.dart.js": true,
        "flutter_service_worker.js": true,
        "flutter.js": true,
        "flutter_bootstrap.js": true,
        "version.json": true,
        ".last_build_id": true,
        "canvaskit/": true,
        "assets/AssetManifest.*": true,
        "assets/FontManifest.json": true,
        "assets/NOTICES": true
    },
    "files.watcherExclude": {
        "**/.dart_tool/**": true,
        "**/build/**": true
    },
    "editor.rulers": [80],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "files.associations": {
        "*.dart": "dart"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "pubspec.yaml": "pubspec.lock",
        "*.dart": "${capture}.freezed.dart, ${capture}.g.dart, ${capture}.gr.dart"
    },
    "dart.openDevTools": "flutter",
    "dart.devToolsTheme": "dark"
} 